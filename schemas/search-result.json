{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "domain": {
      "type": "string"
    },
    "domain_utf8": {
      "type": "string"
    },
    "available": {
      "type": "boolean"
    },
    "price": {
      "type": "object",
      "properties": {
        "USD": {
          "type": "integer"
        },
        "EUR": {
          "type": "integer"
        },
        "NZD": {
          "type": "integer"
        }
      }
    },
    "caveats": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "reason": {
            "type": "string"
          }
        }
      }
    }
  }
}
